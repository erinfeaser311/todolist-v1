<%- include("header") -%>

  <div class="box">
    <% for(var i = 0; i < newListItem.length; i++) { %>
    <div class="item">
      <input type="checkbox" />
      <p><%= newListItem[i] %></p>
    </div>
    <% } %>

    <form class="item" action="/" method="post">
      <input type="text" name="newItem" placeholder="New Item" autocomplete="false" />
      <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
  </div>

<%- include("footer") -%>
